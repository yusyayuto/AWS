Sub DeleteCols_Blockwise()
    Const BLOCK_SIZE As Long = 33    ' A～AG
    Const DELETE_FROM As Long = 8    ' H
    Const DELETE_TO   As Long = 33   ' AG
    
    Dim ws As Worksheet
    Set ws = ThisWorkbook.Worksheets("Sheet2") ' シート名は調整
    
    Dim lastCol As Long
    lastCol = ws.Cells(1, ws.Columns.Count).End(xlToLeft).Column
    
    Dim b As Long, sCol As Long, eCol As Long
    Dim lastBlock As Long
    lastBlock = ((lastCol - 1) \ BLOCK_SIZE) + 1
    
    ' 右から削除
    For b = lastBlock To 1 Step -1
        sCol = (b - 1) * BLOCK_SIZE + DELETE_FROM
        eCol = (b - 1) * BLOCK_SIZE + DELETE_TO
        If sCol <= lastCol Then
            If eCol > lastCol Then eCol = lastCol
            ws.Range(ws.Columns(sCol), ws.Columns(eCol)).EntireColumn.Delete
            lastCol = ws.Cells(1, ws.Columns.Count).End(xlToLeft).Column
        End If
    Next b
End Sub
